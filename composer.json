{
    "name": "kfoobar/laravel-smart-sitemap",
    "description": "A Laravel package for generating dynamic sitemaps",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "illuminate/config": ">=7.0",
        "illuminate/http": ">=7.0",
        "illuminate/support": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "KFoobar\\SmartSitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KFoobar\\SmartSitemap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KFoobar\\SmartSitemap\\SitemapServiceProvider"
            ],
            "aliases": {
                "SmartSitemap": "KFoobar\\SmartSitemap\\Facades\\Sitemap"
            }
        }
    },
    "authors": [
        {
            "name": "David Villa",
            "email": "david@kfoobar.se"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
